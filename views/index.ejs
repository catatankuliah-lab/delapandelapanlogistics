<!DOCTYPE html>
<html lang="en" class="light-style layout-menu-fixed" dir="ltr" data-theme="theme-default" data-assets-path="../assets/"
    data-template="vertical-menu-template-free">

<head>
    <meta charset="utf-8" />
    <meta name="viewport"
        content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0" />
    <title><%= title %></title>

    <!-- Favicon -->
    <link rel="icon" type="image/x-icon" href="img/icon.png" />

    <!-- Bootstrap CSS -->
    <link href="css/bootstrap.min.css" rel="stylesheet">

    <!-- Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
        href="https://fonts.googleapis.com/css2?family=Public+Sans:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&display=swap"
        rel="stylesheet" />

    <!-- Icons. Uncomment required icon fonts -->
    <link rel="stylesheet" href="fonts/boxicons.css" />

    <!-- Core CSS -->
    <link rel="stylesheet" href="css/core.css" class="template-customizer-core-css" />
    <link rel="stylesheet" href="css/theme-default.css" class="template-customizer-theme-css" />

    <!-- Perfect-Scrollbar CSS -->
    <link rel="stylesheet" href="libs/perfect-scrollbar/perfect-scrollbar.css" />

    <!-- Helpers -->
    <script src="js/helpers.js"></script>
    <script src="js/config.js"></script>

    <!-- JQuery -->
    <script src="js/jquery.js"></script>

    <!-- SWAL 2 -->
    <link rel="stylesheet" href="css/sweetalert2.css">
    <script src="js/sweetalert2.js"></script>
</head>

<body>
    <!-- Layout wrapper -->
    <div class="layout-wrapper layout-content-navbar">
        <div class="layout-container">
            <!-- Menu -->
            <aside id="layout-menu" class="layout-menu menu-vertical menu bg-menu-theme">
                <div class="app-brand demo mt-3">
                    <a href="" class="app-brand-link">
                        <img src="img/logos/logo.png" alt="" width="200px">
                    </a>
                    <a href="javascript:void(0);"
                        class="layout-menu-toggle menu-link text-large ms-auto d-block d-xl-none">
                        <i class="bx bx-chevron-left bx-sm align-middle"></i>
                    </a>
                </div>
                <div class="menu-inner-shadow"></div>
                <ul class="menu-inner py-1" id="navigasi">
                    <li class="menu-header small text-uppercase">
                        <span class="menu-header-text">Dashboard</span>
                    </li>
                    <li class="menu-item" id="dashboard">
                        <a id="buttondashboard" class="menu-link" href="javascript:void(0)">
                            <i class="menu-icon tf-icons bx bx-home-circle"></i>
                            <div id="dsh" data-i18n="Analytics">Dashboard</div>
                        </a>
                    </li>
                    <li class="menu-header small text-uppercase">
                        <span class="menu-header-text">Mobil dan Driver</span>
                    </li>
                    <li class="menu-item" id="mobil">
                        <a id="buttonmobil" class="menu-link" href="javascript:void(0)">
                            <i class="menu-icon tf-icons bx bx-car"></i>
                            <div data-i18n="Analytics">Mobil</div>
                        </a>
                    </li>
                    <li class="menu-item" id="driver">
                        <a id="buttondriver" class="menu-link" href="javascript:void(0)">
                            <i class="menu-icon tf-icons bx bx-user"></i>
                            <div data-i18n="Analytics">Driver</div>
                        </a>
                    </li>
                    <li class="menu-header small text-uppercase">
                        <span class="menu-header-text">Maintenance</span>
                    </li>
                    <!-- Dashboard -->
                    <li class="menu-item" id="tambahmaintenance">
                        <a id="buttontambahmaintenance" class="menu-link" href="javascript:void(0)">
                            <i class="menu-icon tf-icons bx bx-cog"></i>
                            <div data-i18n="Analytics">Tambah Maintenance</div>
                        </a>
                    </li>
                    <li class="menu-item" id="riwayatmaintenance">
                        <a id="buttonriwayatmaintenance" class="menu-link" href="javascript:void(0)">
                            <i class="menu-icon tf-icons bx bx-calendar-check"></i>
                            <div data-i18n="Analytics">Riwayat Maintenance</div>
                        </a>
                    </li>
                    <li class="menu-item mt-auto mb-3">
                        <a id="buttonlogout" class="menu-link bg-primary text-white justify-content-center"
                            href="javascript:void(0)">
                            <i class="menu-icon tf-icons bx bx-log-in-circle"></i>
                            <div data-i18n="Basic">Logout</div>
                        </a>
                    </li>
                </ul>
            </aside>
            <!-- Layout container -->
            <div class="layout-page">
                <!-- Navbar -->
                <nav class="hide-navbar layout-navbar container-xxl navbar navbar-expand-xl navbar-detached justify-content-start align-items-center bg-navbar-theme"
                    id="layout-navbar" style="margin-bottom: -20px;">
                    <div class="layout-menu-toggle navbar-nav align-items-xl-center me-3 me-xl-0 d-xl-none">
                        <a class="nav-item nav-link px-0 me-xl-4" href="javascript:void(0)">
                            <i class="bx bx-menu bx-sm"></i>
                        </a>
                    </div>
                    <div class="navbar-nav align-items-xl-start me-xl-0 d-xl-none fw-bolder">
                        <span id="namapage"></span>
                    </div>
                    <div class="d-flex align-items-center ms-auto" id="navbar-collapse">
                        <ul class="navbar-nav flex-row align-items-center ms-auto">
                            <!-- User -->
                            <li class="nav-item navbar-dropdown dropdown-user dropdown">
                                <a class="nav-link dropdown-toggle hide-arrow" href="javascript:void(0);"
                                    data-bs-toggle="dropdown">
                                    <div class="avatar">
                                        <img src="img/logos/user.png" alt class="w-px-4 h-auto " />
                                    </div>
                                </a>
                            </li>
                            <!--/ User -->
                        </ul>
                    </div>
                </nav>
                <!-- / Navbar -->

                <!-- Content wrapper -->

                <div class="content-wrapper">
                    <div id="konten"></div>
                    <!-- Content -->

                    <!-- / Content -->
                    <!-- Footer -->
                    <footer class="content-footer footer bg-footer-theme">
                        <div
                            class="container-xxl d-flex flex-wrap justify-content-between py-2 flex-md-row flex-column">
                            <div class="mb-3 text-primary fw-bolder text-center">
                                Â©
                                <script>
                                    document.write(new Date().getFullYear());
                                </script>, kalaitu.dev x delapandelapanlogistics
                            </div>
                        </div>
                    </footer>
                    <!-- / Footer -->

                    <div class="content-backdrop fade"></div>
                </div>
                <!-- Content wrapper -->
            </div>
            <!-- / Layout page -->
        </div>

        <!-- Overlay -->
        <div class="layout-overlay layout-menu-toggle"></div>
    </div>
    <!-- / Layout wrapper -->
    <!-- Bootstrap -->
    <script src="js/bootstrap.bundle.min.js"></script>
    <!-- Perfect Scrollbar -->
    <script src="libs/perfect-scrollbar/perfect-scrollbar.js"></script>
    <!-- Menu -->
    <script src="js/menu.js"></script>
    <!-- Main JS -->
    <script src="js/main.js"></script>
    <!-- Place this tag in your head or just before your close body tag. -->
    <script async defer src="https://buttons.github.io/buttons.js"></script>
    <!-- main js custom-->
    <!-- Place this tag in your head or just before your close body tag. -->
    <script defer src="js/mainjs.js?v=<%= new Date().getTime() %>"></script>

</body>

</html>